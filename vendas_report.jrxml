	<queryString>
		<![CDATA[select v.id, c.nome, sum(vp.qtde * p.preco) as total
from vendas v
inner join clientes c on c.id = v.id_cliente
inner join vendas_produtos vp on v.id = vp.id_venda
inner join produtos p on p.id = vp.id_produto
group by 1,2]]>
	</queryString>
